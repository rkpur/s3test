{
	"name": "DS_ADLS_Blob",
	"properties": {
		"linkedServiceName": {
			"referenceName": "LS_AzureBlobStorage",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"P_container_name": {
				"type": "string",
				"defaultValue": "blob-s3"
			},
			"P_folder": {
				"type": "string",
				"defaultValue": "src"
			},
			"P_file_name": {
				"type": "string",
				"defaultValue": "*.*"
			}
		},
		"annotations": [],
		"type": "Binary",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@dataset().P_file_name",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@dataset().P_folder",
					"type": "Expression"
				},
				"container": {
					"value": "@dataset().P_container_name",
					"type": "Expression"
				}
			}
		}
	}
}